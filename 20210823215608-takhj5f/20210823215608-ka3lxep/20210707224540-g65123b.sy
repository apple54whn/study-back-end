								{
									"Type": "NodeTextMark",
									"TextMarkType": "strong",
									"TextMarkTextContent": "面向缓冲区"
								},
								{
									"Type": "NodeText",
									"Data": "的(IO是面向流的)、"
								},
								{
									"Type": "NodeTextMark",
									"TextMarkType": "strong",
									"TextMarkTextContent": "基于通道"
								},
								{
									"Type": "NodeText",
									"Data": "的IO操作。NIO将以更加高效的方式进行文件的读写操作。"
								}
							]
						}
					]
